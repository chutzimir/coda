Server Prefix "S";
Subsystem "volUtil";

/* The portal for volume utility requests to the file server */
/* Must exist in /etc/services */
#define UTIL_PORTAL "dsys2"

/* subsystem id for volume utility requests to the file server */
#define UTIL_SUBSYSID	7777

#define VolName	"VolumeUtility"		/* identity of volume utility requests */
#define TKFile  "/vice/db/volutil.tk"	/* name of token key file */

#include "vcrcommon.rpc2"

typedef RPC2_Struct {
    RPC2_Integer nLargeVnodes;
    RPC2_Integer nSmallVnodes;
    RPC2_Integer DirPagesSize;
    RPC2_Integer LargeVnodeSize;
    RPC2_Integer SmallVnodeSize;
    RPC2_Integer VolumeSize;
} RVMSize_data;

VolSalvage(IN RPC2_String partition, IN RPC2_Unsigned VolumeNumber, IN RPC2_Integer force, IN RPC2_Integer debug, IN RPC2_Integer list);

VolCreate(IN RPC2_String partition, IN RPC2_String volname, IN OUT VolumeId volid, IN RPC2_Integer repvol, IN VolumeId grpid);

VolInfo(IN RPC2_String volname, IN RPC2_Integer all, IN OUT SE_Descriptor sed);

VolMakeVLDB(IN RPC2_String infile);

VolMakeVRDB(IN RPC2_String infile);

VolShowVnode(IN RPC2_Unsigned volnumber, IN RPC2_Unsigned vnodeNumber, IN RPC2_Unsigned unique, IN OUT SE_Descriptor sed);

VolSetVV(IN RPC2_Unsigned volnumber, IN RPC2_Unsigned vnodeNumber, IN RPC2_Unsigned unique, IN  ViceVersionVector vv);

VolPurge(IN RPC2_Unsigned volid, IN RPC2_String volname);

VolLookup(IN RPC2_String vol, IN OUT SE_Descriptor sed);

VolUpdateDB();

VolShutdown();

VolSwaplog();

VolSetDebug(IN RPC2_Integer debuglevel);

VolDump();	/* Place holder, replaced by next call. */

VolNewDump(IN RPC2_Unsigned volid, IN OUT RPC2_Unsigned Incremental);

VolClone(IN RPC2_Unsigned ovolid, IN RPC2_String newvolname, OUT RPC2_Unsigned cloneId);

VolRestore(IN RPC2_String partname, IN RPC2_String volname, IN OUT RPC2_Unsigned volid);

VolMerge();

VolMakeBackups(IN VolumeId Vid, OUT VolumeId BackupVid);

VolMarkAsAncient(IN VolumeId GroupId, IN VolumeId RepId);   /* In vol-backup.c */

/* VolChkRec(IN RPC2_Unsigned volid); */

VolDumpMem(IN  RPC2_String filename, IN RPC2_Unsigned address, IN RPC2_Unsigned size);

/* VolDumpRecStore(IN RPC2_Unsigned volid, IN RPC2_String fname); */

VolLock(IN VolumeId Vid, OUT ViceVersionVector VolVV);

VolUnlock(IN VolumeId Vid);

VolRVMSize(IN VolumeId VolID, OUT RVMSize_data data);

VolSetLogParms(IN VolumeId Vid, IN RPC2_Integer OnFlag, IN RPC2_Integer maxentries);

VolTiming(IN RPC2_Integer on, IN OUT SE_Descriptor sed);

VolElapse(IN RPC2_Integer on, IN RPC2_Integer subid, IN RPC2_Integer multi);

PrintStats(IN OUT SE_Descriptor sed);

TraceRpc(IN OUT SE_Descriptor sed);

ShowCallbacks	(IN ViceFid fid,
		 IN OUT SE_Descriptor sed);

TruncateRVMLog();

VolGetMaxVolId(OUT RPC2_Integer maxid);

VolSetMaxVolId(IN RPC2_Integer newid);

VolPeekInt(IN RPC2_String address, OUT RPC2_Integer pvalue);

VolPokeInt(IN RPC2_String address, IN RPC2_Integer value);

VolPeekMem(IN RPC2_String address, IN OUT RPC2_BoundedBS buf);

VolPokeMem(IN RPC2_String address, IN RPC2_CountedBS buf);

VolSwapmalloc();
