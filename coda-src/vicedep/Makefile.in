TOPDIR = @top_srcdir@

srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

include $(TOPDIR)/Makeconf

INCLPATH= .:$(srcdir)
INCLFLAGS = -I. -I$(srcdir)
CFLAGS := -g -MD -I. -I$(srcdir)


HEADERS = venusioctl.h srv.h operations.h recov_vollog.h

RP2COMP = callback.rpc2 res.rpc2   vice.rpc2 volutil.rpc2 \
	      voldump.rpc2 mond.rpc2 adsrv.rpc2 admon.rpc2
RP2CLOBJ = $(notdir ${RP2COMP:.rpc2=.client.o})
RP2SROBJ = $(notdir ${RP2COMP:.rpc2=.server.o})
RP2MUOBJ = $(notdir ${RP2COMP:.rpc2=.multi.o})

RP2FILES    = $(RP2COMP) vcrcommon.rpc2 
RP2HEADERS  = $(notdir ${RP2FILES:.rpc2=.h})


LIBRARIES = libvicedep.a



vcrcommon.h: vcrcommon.rpc2

callback.client.c: vcrcommon.h

libvicedep.a: $(RP2CLOBJ) $(RP2SROBJ)  $(RP2MUOBJ)

include $(TOPDIR)/configs/Makerules
