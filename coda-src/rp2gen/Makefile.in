# The REAL Makefile
# The other Makefiles invoke this after setting: CC CFLAGS LFLAGS
TOPDIR = @top_srcdir@

srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

include $(TOPDIR)/Makeconf

CC=$(NATIVECC)
CFLAGS:= -MD -I. -I/usr/include -I$(TOPOBJ)/include 

EXECUTABLES = rp2gen

YFLAGS = -d

include $(TOPDIR)/configs/Makerules

# All the things that seem to depend on y.tab.c (except y.tab.o) really depend
# on y.tab.h, but it gets generated at the same time by yacc...

rp2gen: rp2main.o rp2gen_parser.o rp2gen_lexer.o symtab.o crout.o util.o $(LIBFIXUP)
	${CC} $(LIBFLAGS) rp2main.o rp2gen_parser.o rp2gen_lexer.o symtab.o crout.o util.o $(LIBFIXUP) -o rp2gen

rp2main.o: rp2main.c rp2.h

rp2gen_parser.o: rp2gen_parser.c rp2.h

rp2gen_lexer.o: rp2gen_lexer.c rp2.h

symtab.o: symtab.c rp2.h

crout.o: crout.c rp2.h

util.o	: util.c rp2.h

# these need to be cleaned too
TESTS= rp2gen_lexer.c rp2gen_parser.c y.tab.h
