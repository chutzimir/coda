# The REAL Makefile
# The other Makefiles invoke this after setting: CC CFLAGS LFLAGS


SOURCES = crout.c rp2gen.h rp2.h rp2gen.mss rp2gen.yacc rp2gen.lex rp2main.c symtab.c test.c test.rpc2 util.c

# All the things that seem to depend on y.tab.c (except y.tab.o) really depend
# on y.tab.h, but it gets generated at the same time by yacc...

all: rp2gen

rp2gen: rp2main.o y.tab.o lex.yy.o symtab.o crout.o util.o
	${CC} $(CFLAGS) rp2main.o y.tab.o lex.yy.o symtab.o crout.o util.o -o rp2gen

rp2main.o: rp2main.c rp2.h

y.tab.o: y.tab.c rp2.h

y.tab.c: rp2gen.yacc
	yacc -d rp2gen.yacc

lex.yy.o: lex.yy.c rp2.h

lex.yy.c: rp2gen.lex
	lex $(LFLAGS) rp2gen.lex

symtab.o: symtab.c rp2.h

crout.o: crout.c rp2.h

util.o	: util.c rp2.h

clean:
	-rm -rf *.o rp2gen lex.yy.c y.tab.*

.INOBJECTDIR: rp2.h
	cp -p $< ./$<
	chmod 0644 ./$<
