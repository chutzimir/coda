  VI = auth2 vv login
#endif
 VII =  vol resolve
VIII =  venus advice repair res
  IX =  rvmres volutil vtools
   X =  vice
  XI =  update norton asr egasr
# If you add a new module to Coda, add it to the right set above

SUBDIRS = $(I) $(II) $(III) $(IV) $(V) $(VI) $(VII) $(VIII) $(IX) \
	  $(X) $(XI)

include $(TOPDIR)/Makerules



