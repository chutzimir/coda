TOPDIR = @top_srcdir@

srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

include $(TOPDIR)/Makeconf

EXECUTABLES = sidekick
CSBINS      = $(EXECUTABLES)
LIBRARIES   = 
HEADERS	    = adv_sidekick.h
CCONFFILES  = sidekick.intr.ex

DEPLIBS := $(LIBUTIL) $(LIBVICEDEP) $(LIBCLNREPAIR) $(LIBREPIO) $(LIBVV) \
	$(LIBAUTH2) $(LIBKERNDEP) $(LIBUTIL) $(LIBAUSER) $(LIBBASE)
LIBS := $(DEPLIBS) $(LIBRPC2) $(LIBLWP) $(LIBSYS) \
	$(LIBKVM) $(LIBREADLINE) $(LIBTERMCAP) $(LIBCRYPTO) $(LIBS)

OBJS = adv_sidekick.o

sidekick: $(HEADERS) $(DEPLIBS) $(OBJS)
	$(CXX) $(LDFLAGS) $(OBJS) $(LIBS) -o sidekick

include $(TOPDIR)/configs/Makerules
