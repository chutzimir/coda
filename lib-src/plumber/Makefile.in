TOPDIR = @top_srcdir@
ifndef MAKDIR
MAKDIR = @top_srcdir@
endif
srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

include $(MAKDIR)/Makeconf

LIBRARIES = libplumber.a

libplumber.a: pmalloc.o getcallr.o pplumber.o

getcallr.o: getcallr.spp
	$(CPP)  -I/usr/include getcallr.spp getcallr.ss
	as -o getcallr.o getcallr.ss
	rm getcallr.ss

malloc.o: malloc.ci
plumber.o: plumber.ci
pmalloc.o: malloc.ci
pplumber.o: plumber.ci

include $(MAKDIR)/Makeconf
