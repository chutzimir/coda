
TOPDIR = @top_srcdir@
ifndef MAKDIR
MAKDIR = @top_srcdir@
endif
srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

# Need to conditionalize this
include $(MAKDIR)/Makeconf

EXECUTABLES = mount unmount relay
CSBINS = mount unmount relay

mount: mount.o 
	$(CC) -o mount mount.o 

unmount: unmount.o 
	$(CC) -o unmount unmount.o 

relay: relay.o
	$(CC) -o relay relay.o


include $(MAKDIR)/configs/Makerules


