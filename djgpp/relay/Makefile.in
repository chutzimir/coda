
TOPDIR = @top_srcdir@

srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

# Need to conditionalize this
include $(TOPDIR)/Makeconf

HEADERS = relay.h vxd.h
LIBRARIES = librelay.a
EXECUTABLES = mount unmount 
CSBINS = mount unmount 

OBJS= relay.o vxd.o

librelay.a:	$(OBJS)

mount: mount.o vxd.o 
	$(CC) -o mount mount.o vxd.o

unmount: unmount.o vxd.o
	$(CC) -o unmount unmount.o vxd.o 

relay: relay.o vxd.o
	$(CC) -o relay relay.o vxd.o


include $(TOPDIR)/configs/Makerules


