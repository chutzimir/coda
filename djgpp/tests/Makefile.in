
TOPDIR = @top_srcdir@

srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

# Need to conditionalize this
include $(TOPDIR)/Makeconf

EXECUTABLES = hello testudp selectme theabort
CSBINS = hello testudp selectme

hello: hello.o
	$(CC) $(LDFLAGS) -o hello hello.o

theabort: theabort.o
	$(CC) $(LDFLAGS) -o theabort theabort.o

testudp: testudp.o
	$(CC) $(LDFLAGS) -o testudp testudp.o

selectme: selectme.o
	$(CC) $(LDFLAGS) -o selectme selectme.o

include $(TOPDIR)/configs/Makerules


